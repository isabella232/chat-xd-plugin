{
  "name": "xd-plugin-boilerplate",
  "version": "0.0.1",
  "description": "A simple plugin, built with the xd-webpack-starter-template package",
  "private": true,
  "scripts": {
    "start": "npm run debug:watch",
    "build": "npm run compile && xdpm package dist",
    "compile": "webpack --config webpack.config.js --mode production",
    "debug:install": "webpack --config webpack.config.js --mode production && xdpm install dist -o",
    "debug:watch": "xdpm watch dist | webpack --config webpack.config.js --mode production --watch",
    "debug:watch-prerelease": "xdpm watch dist -w p | webpack --config webpack.config.js --mode production --watch",
    "lint": "eslint src/**/*.js --ignore-pattern node_modules/",
    "stats": "sloc src",
    "docs": "jsdoc -r src -d docs"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "v4-uuid": "^1.1.0",
    "xd-storage-helper": "^1.2.3"
  },
  "devDependencies": {
    "@adobe/xdpm": "^4.0.0",
    "@types/adobe-xd": "AdobeXD/typings",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "file-loader": "^6.0.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "sloc": "^0.2.1",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "keywords": [
    "adobe",
    "xd"
  ],
  "author": "Pablo Klaschka",
  "license": "MIT"
}
